{
  "name": "vue-laboratory",
  "description": "jue.js + Bootstrapの実験室",
  "version": "1.0.0",
  "author": "multios12 <multiple12@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/multios12/vue-laboratory.git"
  },
  "private": true,
  "scripts": {
    "dev": "run-p watch.* start",
    "start": "node dist/server.js",
    "build": "cross-env NODE_ENV=development webpack && tsc",
    "release": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "watch": "run-p watch.*",
    "watch.ts": "nodemon --ext ts --watch src -x tsc",
    "watch.bundle": "nodemon --ext js,html,vue --watch src/public -x cross-env NODE_ENV=development webpack"
  },
  "dependencies": {
    "bootstrap-vue": "^2.0.0-rc.8",
    "compression": "^1.7.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "^1.9.0",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-solid": "^5.0.9",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.11.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "browser-sync": "^2.23.7",
    "connect-browser-sync": "^2.1.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "~1.17.2",
    "npm-run-all": "^4.1.2",
    "tar": "~4.4.1",
    "typescript": "^2.8.3",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
